{
  "name": "github-unfollow",
  "version": "1.0.0",
  "description": "Unfollow github users that don't follow you back.",
  "repository": "https://github.com/eingress/github-unfollow.git",
  "author": "Scott Mathieson <scott@eingress.io>",
  "license": "ISC",
  "keywords": [
    "github",
    "node",
    "unfollow"
  ],
  "scripts": {
    "build": "yarn build:clean && babel src -d dist",
    "build:clean": "rimraf dist/*",
    "build:watch": "yarn build -w",
    "coverage": "jest --collectCoverageFrom=src/**/*.js --coverage",
    "lint": "eslint './{,{__tests__,src}/**/}*.js' --fix --quiet",
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "start:dev": "nodemon src/index.js",
    "test": "jest --silent",
    "test:watch": "jest --silent --watch"
  },
  "dependencies": {
    "dotenv": "16.0.3"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "7.18.10",
    "@babel/eslint-parser": "7.18.9",
    "@babel/node": "7.18.10",
    "@babel/preset-env": "7.18.10",
    "eslint": "8.21.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.3",
    "nodemon": "2.0.19",
    "prettier": "2.7.1",
    "rimraf": "3.0.2"
  }
}
